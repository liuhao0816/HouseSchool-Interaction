<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gxa.mapper.SchoolMapper">


    <select id="selectAllByName"  parameterType="map" resultType="com.gxa.entity.School">
        select * from `school`
        <where>
            <if test="school_name!=null and school_name!=''">
                `school_name` like concat('%',#{school_name},'%')
            </if>
        </where>
    </select>

</mapper>